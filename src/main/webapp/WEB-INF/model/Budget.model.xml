<?xml version="1.0" encoding="utf-8" ?>

<model xmlns="http://www.top-logic.com/ns/dynamic-types/6.0">
	<module name="Budget">
		<enum name="Energieklasse">
			<classifier name="APlus"/>
			<classifier name="A"/>
			<classifier name="B"/>
			<classifier name="C"/>
			<classifier name="D"/>
			<classifier name="E"/>
			<classifier name="F"/>
			<classifier name="G"/>
			<classifier name="H"/>
		</enum>
		<enum name="Prioritaet">
			<classifier name="minor"/>
			<classifier name="major"/>
			<classifier name="critical"/>
		</enum>
		<class name="Anbietertrick">
			<generalizations>
				<generalization type="tl.model:TLObject"/>
			</generalizations>
			<attributes>
				<property name="bezeichnung"
					type="tl.core:String"
				/>
				<reference name="prioritaet"
					kind="forwards"
					navigate="true"
					type="Prioritaet"
				/>
			</attributes>
		</class>
		<class name="Wohnung">
			<generalizations>
				<generalization type="tl.model:TLObject"/>
			</generalizations>
			<attributes>
				<property name="url"
					type="tl.core:String"
				>
					<annotations>
						<size-constraint upper-bound="10000"/>
					</annotations>
				</property>
				<property name="kaltmiete"
					type="tl.core:Integer"
				/>
				<property name="adresse"
					type="tl.core:String"
				/>
				<property name="flaeche"
					type="tl.core:Integer"
				/>
				<property name="nebenkosten"
					type="tl.core:Integer"
				/>
				<property name="zzglHeizkosten"
					type="tl.core:Boolean"
				/>
				<property name="energiebedarf"
					type="tl.core:Integer"
				/>
				<property name="zimmeranzahl"
					type="tl.core:Integer"
				/>
				<reference name="energieklasse"
					kind="forwards"
					type="Energieklasse"
				/>
				<property name="bezeichnung"
					type="tl.core:String"
				/>
				<reference name="vorteile"
					kind="forwards"
					multiple="true"
					navigate="true"
					type="Wohnungsmerkmal"
				/>
				<reference name="nachteile"
					kind="forwards"
					multiple="true"
					navigate="true"
					type="Wohnungsmerkmal"
				/>
				<property name="aktiv"
					mandatory="true"
					type="tl.core:Boolean"
				/>
				<reference name="anbietertricks"
					kind="forwards"
					multiple="true"
					navigate="true"
					type="Anbietertrick"
				/>
				<reference name="anbieter"
					kind="forwards"
					navigate="true"
					type="Wohnungsanbieter"
				/>
				<property name="miete"
					type="tl.core:Integer"
				>
					<annotations>
						<storage-algorithm>
							<query expr="wohnung->$wohnung.get(`Budget:Wohnung#kaltmiete`)+$wohnung.get(`Budget:Wohnung#nebenkosten`)"/>
						</storage-algorithm>
					</annotations>
				</property>
			</attributes>
		</class>
		<class name="Wohnungsanbieter">
			<generalizations>
				<generalization type="tl.model:TLObject"/>
			</generalizations>
			<attributes>
				<property name="name"
					mandatory="true"
					type="tl.core:String"
				/>
				<property name="wahrheitswert"
					type="tl.core:Double"
				>
					<annotations>
						<storage-algorithm>
							<query>
								<expr><![CDATA[anbieter->{
scala = 5;
wohnungen = $anbieter.referers(`Budget:Wohnung#anbieter`);
summe = $wohnungen.get(`Budget:Wohnung#anbietertricks`).map(tricks->max(0,$scala-$tricks.size())).sum()/$scala;
$summe/$wohnungen.size();
}]]></expr>
							</query>
						</storage-algorithm>
					</annotations>
				</property>
			</attributes>
		</class>
		<class name="Wohngesellschaft">
			<generalizations>
				<generalization type="Wohnungsanbieter"/>
			</generalizations>
			<attributes>
				<reference name="mitarbeiter"
					kind="forwards"
					multiple="true"
					navigate="true"
					type="Wohnungsanbieter"
				/>
				<property name="wahrheitswert"
					override="true"
					type="tl.core:Double"
				>
					<annotations>
						<storage-algorithm>
							<query expr="gesellschaft->$gesellschaft.get(`Budget:Wohngesellschaft#mitarbeiter`).map(mitarbeiter->$mitarbeiter.get(`Budget:Wohnungsanbieter#wahrheitswert`)).average()"/>
						</storage-algorithm>
					</annotations>
				</property>
			</attributes>
		</class>
		<class name="Wohnungsmerkmal">
			<generalizations>
				<generalization type="tl.model:TLObject"/>
			</generalizations>
			<attributes>
				<property name="bezeichnung"
					type="tl.core:String"
				/>
				<reference name="prioritaet"
					kind="forwards"
					navigate="true"
					type="Prioritaet"
				/>
			</attributes>
		</class>
	</module>
</model>